<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PendingBills">


        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar4"
            android:layout_width="414dp"
            android:layout_height="53dp"
            android:background="#FD6A02"
            android:minHeight="?attr/actionBarSize"
            android:theme="?attr/actionBarTheme"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="44dp"
        android:layout_height="43dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/nav"
        tools:layout_editor_absoluteY="7dp" />

    <TextView
        android:id="@+id/Head2"
        android:layout_width="164dp"
        android:layout_height="28dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="72dp"
        android:text="@string/head"
        android:textColor="#ffc581"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/toolbar4"
        tools:layout_editor_absoluteY="7dp" />

    <TextView
        android:id="@+id/BillCategory"
        android:layout_width="112dp"
        android:layout_height="37dp"
        android:text="@string/line1"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="61dp" />


    <TextView
        android:id="@+id/billCategory"
        android:layout_width="152dp"
        android:layout_height="28dp"
        android:text="@string/line1_1"
        android:textColor="@android:color/tab_indicator_text"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="135dp" />

    <TextView
        android:id="@+id/duedate1"
        android:layout_width="73dp"
        android:layout_height="31dp"
        android:text="@string/line2_1"
        android:textColor="@android:color/holo_red_light"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="166dp" />

    <TextView
        android:id="@+id/duudate2"
        android:layout_width="56dp"
        android:layout_height="24dp"
        android:text="@string/line2"
        android:textColor="@android:color/holo_red_light"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="90dp" />



    <View
        android:id="@+id/divider"
        android:layout_width="413dp"
        android:layout_height="1dp"
        android:background="?android:attr/listDivider"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="125dp" />


</androidx.constraintlayout.widget.ConstraintLayout>




